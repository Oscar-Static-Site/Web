{
	"Version": "2023-7-25",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": ["s3:ListBucket", "s3.GetBucketLocation"],
			"Resource": "arn:aws:s3::www.oscarcorner.com/*"
		},
		{
			"Effect": "Allow",
			"Action": "s3:GetObject",
			"Resource": "arn:aws:s3::www.oscarcorner.com/*"
			"Action":[
            "s3:PutObject",
            "s3:PutObjectAcl",
            "s3:DeleteObject"
         ],
		},
	]
}
